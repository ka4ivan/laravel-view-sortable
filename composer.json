{
    "name": "ka4ivan/laravel-view-sortable",
    "description": "Sort Link and URL for Laravel Framework",
    "keywords": [
        "sort", "link", "url", "laravel", "html-sort", "kachula"
    ],
    "authors": [
        {
            "name": "Kachula Ivan",
            "email": "ka4ivan.dev@gmail.com",
            "role": "Architect & Developer",
            "homepage": "https://github.com/ka4ivan"
        }
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Ka4ivan\\ViewSortable\\": "src"
        },
        "files": [
            "src/Support/Sort.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Ka4ivan\\ViewSortable\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ka4ivan\\ViewSortable\\ServiceProvider"
            ],
            "aliases": {
                "Sort": "Ka4ivan\\ViewSortable3\\Facades\\Sort"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
